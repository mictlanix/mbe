@using Mictlanix.BE
@using Mictlanix.BE.Model
@using Mictlanix.BE.Web.Helpers
@model Warehouse

<div class="row">
    <div class="span2 field-required">@Html.LabelFor(x => x.Code)</div>
    <div class="span6 field-required">@Html.LabelFor(x => x.Name)</div>
</div>
<div class="row">
    <div class="span2"><div class="ro-field">@Html.DisplayFor(x => x.Code)</div></div>
    <div class="span6"><div class="ro-field">@Html.DisplayFor(x => x.Name)</div></div>
</div>
<div class="row">
    <div class="span8">@Html.LabelFor(x => x.Store)</div>
</div>
<div class="row">
    <div class="span8"><div class="ro-field">@Html.DisplayFor(x => x.Store.Name)</div></div>
</div>
<div class="row">
    <div class="span8">@Html.LabelFor(x => x.Comment)</div>
</div>
<div class="row">
    <div class="span8"><div class="ro-field">@Html.DisplayFor(x => x.Comment)</div></div>
</div>

