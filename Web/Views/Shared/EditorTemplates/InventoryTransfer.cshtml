@using Mictlanix.BE
@using Mictlanix.BE.Model
@model InventoryTransfer

<div class="form-group">
    <div class="col-xs-12 field-required">@Html.LabelFor(x => x.From) @Html.EditorFor(x => x.FromId) @Html.ValidationMessageFor(x => x.FromId)</div>
</div>
<div class="form-group">
    <div class="col-xs-12 field-required">@Html.LabelFor(x => x.To) @Html.EditorFor(x => x.ToId) @Html.ValidationMessageFor(x => x.ToId)</div>
</div>
<div class="form-group">
    <div class="col-xs-12">@Html.LabelFor(x => x.Comment) @Html.EditorFor(x => x.Comment)</div>
</div>