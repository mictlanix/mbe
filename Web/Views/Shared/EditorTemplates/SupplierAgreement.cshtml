@using Mictlanix.BE
@using Mictlanix.BE.Model
@model SupplierAgreement

@Html.HiddenFor(x => x.SupplierId)
<div class="form-group">
    <div class="col-xs-6 field-required">@Html.LabelFor(x => x.Start) @Html.EditorFor(x => x.Start) @Html.ValidationMessageFor(x => x.Start)</div>
    <div class="col-xs-6 field-required">@Html.LabelFor(x => x.End) @Html.EditorFor(x => x.End) @Html.ValidationMessageFor(model => model.End)</div>
</div>
<div class="form-group">
    <div class="col-xs-12">@Html.LabelFor(x => x.Comment) @Html.EditorFor(x => x.Comment)</div>
</div>