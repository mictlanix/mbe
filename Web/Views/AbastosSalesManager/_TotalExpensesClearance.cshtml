@using Mictlanix.BE
@using Mictlanix.BE.Model
@using Mictlanix.BE.Web.Helpers
@using Mictlanix.BE.Web.Models
@model PurchaseClearanceDetailSummary

<div class="col-sm-push-12">
				<ul class='labeled-frames pull-right' id="expenses_clearance">
								<li>
												<span class='caption'>@Resources.Quantity</span>
												<span class='value'>@Model.SoldQuantity</span>
