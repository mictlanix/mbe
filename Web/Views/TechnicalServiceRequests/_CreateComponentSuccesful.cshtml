@using Mictlanix.BE
@using Mictlanix.BE.Model
@using Mictlanix.BE.Web.Helpers
@model TechnicalServiceRequestComponent
@{
    ViewBag.Privilege = (Html.CurrentUser() ?? new User()).GetPrivilege (SystemObjects.TechnicalServiceRequests);
}
@Html.Partial("_Component", Model)
<script type='text/javascript'>
$('#components tr:last').after($('#id\\:@Model.Id'));
$('#component-modal').modal('hide');
bindModal('#id\\:@Model.Id a.open-modal');
</script>
