@using Mictlanix.BE
@model Mictlanix.BE.Model.SupplierAgreement

@{
    ViewBag.Title = Resources.Delete;
}

<h2>@Resources.Delete</h2>

<h3>@Resources.Message_DeleteConfirmation</h3>
<fieldset>
    <legend>@Resources.Title_SupplierAgreement</legend>

   

</fieldset>
@using (Html.BeginForm()) {
    <p>
        <button type="submit" class="icon delete">@Resources.Delete</button>
        @Html.ActionLink(Resources.BackToList, "Details", "Suppliers", new { id = Model.SupplierId }, new { @class = "button icon back" })
    </p>
}