@using Mictlanix.BE
@using Mictlanix.BE.Model
@using Mictlanix.BE.Web.Helpers
@model ExchangeRate
@{
    var Privilege = Html.GetPrivilege (Html.CurrentUser () ?? new User (), SystemObjects.ExchangeRates);
    ViewBag.Privilege = Privilege;
}
@Html.Partial("_Item", Model)
<script type='text/javascript'>
$('#exchange-rate tr:first').after($('#id\\:@Model.Id'));
$('#exchangeRate-modal').modal('hide');
bindModal('#id\\:@Model.Id a.open-modal');
</script>
