@using Mictlanix.BE
@model Mictlanix.BE.Models.Kardex

@{
    ViewBag.Title = Resources.Title_Kardex;
}

<h2>@Resources.DisplayName_Kardex</h2> 

<div id="master-section">
    <fieldset>
        <legend>@Resources.DisplayName_WarehouseSelect</legend>
        @using (Html.BeginForm())
        {
            @Html.ValidationSummary(true)
            
            <div class="editor-label">
                @Html.LabelFor(model => model.Warehouse)
                @Html.TextBoxFor(model => model.WarehouseId)
                @Html.ValidationMessageFor(model => model.Warehouse.Name)
            </div>
            
            <p class="right">
                <button type="submit" class="icon save">@Resources.Save</button>
            </p>
        }
    </fieldset>
</div>