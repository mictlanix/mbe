@using Mictlanix.BE
@using Mictlanix.BE.Model
@using Mictlanix.BE.Web.Helpers
@model Warehouse

@{
    var Privilege = Html.GetPrivilege(Html.CurrentUser () ?? new User (), SystemObjects.Warehouses);
    ViewBag.Privilege = Privilege;
}

@Html.Partial("_Item", Model)
<script type='text/javascript'>
$('#warehouses tr:first').after($('#id\\:@Model.Id'));
$('#warehouse-modal').modal('hide');
bindModal('#id\\:@Model.Id a.open-modal');
</script>


