@using Mictlanix.BE
@using Mictlanix.BE.Model
@using Mictlanix.BE.Web.Helpers
@model Product
@{
    var Privilege = Html.GetPrivilege(Html.CurrentUser() ?? new User(), SystemObjects.Products);
    ViewBag.Privilege = Privilege;
}
@Html.Partial("_Item", Model)
<script type='text/javascript'>
$('#products tr:first').after($('#id\\:@Model.Id'));
$('#product-modal').modal('hide');
bindModal('#id\\:@Model.Id a.open-modal');
</script>
