@using Mictlanix.BE
@using Mictlanix.BE.Model
@using Mictlanix.BE.Web.Helpers
@model CashDrawer

@{
    var Privilege = Html.GetPrivilege(Html.CurrentUser () ?? new User (), SystemObjects.CashDrawers);
    ViewBag.Privilege = Privilege;
}
@Html.Partial("_Item", Model)
<script type='text/javascript'>
$('#cash-drawers tr:first').after($('#id\\:@Model.Id'));
$('#cashDrawer-modal').modal('hide');
bindModal('#id\\:@Model.Id a.open-modal');
</script>


