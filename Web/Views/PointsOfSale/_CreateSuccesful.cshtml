@using Mictlanix.BE
@using Mictlanix.BE.Model
@using Mictlanix.BE.Web.Helpers
@model PointOfSale
@{
    var Privilege = Html.GetPrivilege(Html.CurrentUser () ?? new User (), SystemObjects.PointsOfSale);
    ViewBag.Privilege = Privilege;
}
@Html.Partial("_Item", Model)
<script type='text/javascript'>
$('#pointOfSales tr:first').after($('#id\\:@Model.Id'));
$('#pointOfSale-modal').modal('hide');
bindModal('#id\\:@Model.Id a.open-modal');
</script>

